		style.
			@keyframes pop-in {
				0% {opacity: 0; transform: translate(0, 20vh);}
				100% {opacity: 1; transform: translate(0, 0);}
			}
			@keyframes bob {
				0% {transform: translate(0, 0);}
				50% {transform: translate(0, 6vh);}
				100% {transform: translate(0, 0);}
			}
			#triangle-container {
				position: absolute;
				display: flex;
				justify-content: center;
				width: 100vw;
				height: 50vh;
				bottom: 0;
			}
			#triangle {
				position: absolute;
				width: 0;
				height: 0;
				border-left: 50px solid transparent;
				border-right: 50px solid transparent;
				border-top: 50px solid rgba(52, 152, 219, 1);
				bottom: 13vh;
				transform: translate(0, 30vh);
				opacity: 0;
				animation-name: pop-in, bob, pop-in;
				animation-duration: 1s, 2s, 0.5s;
				animation-delay: 0s, 1s, 3s;
				animation-fill-mode: both, forwards, forwards;
				animation-iteration-count: 1, infinite, 1;
				animation-timing-function: ease-out, ease-in-out, ease-out;
				animation-direction: normal, normal, reverse;
			}
			.col-text {
				font-size: 1.25vw;
			}
			@media screen and (max-width: 992px) {
				.col-text {
					font-size: 5vw;
				}
